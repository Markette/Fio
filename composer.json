{
  "name": "contributte/fio",
  "description": "Easy-to-use FIO API integration for Nette Framework",
  "keywords": ["fio", "login", "php", "api", "nette"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/fio",
  "authors": [
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Filip Šuška"
    }
  ],
  "require": {
    "php": ">= 7.1"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.3",
    "nette/di": "^3.0.0",
    "mockery/mockery": "^1.2.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Fio\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
